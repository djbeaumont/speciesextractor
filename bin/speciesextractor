#! /usr/bin/env python3

import sys, os
sys.path.append(os.path.abspath('../speciesextractor'))
import speciesextractor

parser = speciesextractor.Parser('/Users/djb/Desktop/specieswiki-20121213-pages-meta-current.xml')
parser.parse()

sorted_species = sorted(parser.all_species, key=lambda s: s.binomial_name)

inserter = speciesextractor.Inserter()

inserter.insert_locales(parser.locales)
inserter.insert(sorted_species)
